{
  "name": "@tsailab/ioredis-mq",
  "version": "0.1.0",
  "description": "Implementing simple pub/sub based on redis.",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "tsai-plat",
    "nestjs",
    "nestjs-modules",
    "redis",
    "ioredis"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint \"lib/**/*.ts\" --fix",
